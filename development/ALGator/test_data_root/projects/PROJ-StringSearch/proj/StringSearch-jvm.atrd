{
  "ResultDescription":
  { 
    "Format"           : "CSV",
    "Delimiter"        : ";",
    "TestParameters"   : ["Test", "Group", "N"],    	
    "ResultParameters" : ["Tmax", "Tmin", "Tavg", "Tsum", "Check"],    	
  
    "Parameters":   [	
    
      {
       "Name":        "Test",             
       "Description": "The name of the test", 
       "Type":        "string"       
      },
      {
       "Name":        "Group",             
       "Description": "The name of the group to which this test belongs", 
       "Type":        "string"       
      },
      {
       "Name":        "N",             
       "Description": "The size of the test (number of elements to be sorted)", 
       "Type":        "int"       
      },
    
      {
       "Name"         : "Tmax",             
       "Description"  : "Searching MAX time",             
       "Type"         : "timer",          
       "Subtype"      : "0 MAX"    
      },
      {
       "Name"        : "Tmin",             
       "Description" : "Searching MIN time",             
       "Type"        : "timer",          
       "Subtype"     : "0 MIN"    
      },
      {
       "Name"         : "Tavg",             
       "Description"  : "Searching AVG time",             
       "Type"         : "timer",          
       "Subtype"      : "0 AVG"    
      },
      {
       "Name"        : "Tsum",             
       "Description" : "Searching SUM	 time",             
       "Type"        : "timer",          
       "Subtype"     : "0 SUM"    
      },
      {
       "Name"        : "Check",             
       "Description" : "The correctness of the algorithm (OK or NOK)", 
       "Type"        : "string"       
      }
    ]
  }
}
 